<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
    <application>my-life-01</application>
    <version>4</version>
    <threadsafe>true</threadsafe>
    <!-- Sessions enabled https://cloud.google.com/appengine/docs/java/config/appconfig -->
    <sessions-enabled>true</sessions-enabled>
    <!-- reduce latency https://cloud.google.com/appengine/docs/java/config/appconfig -->
    <async-session-persistence enabled="true" />
    <static-files>
        <exclude path="/css/*.*" />
    </static-files>
</appengine-web-app>