<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xmlns="http://java.sun.com/xml/ns/javaee"
           xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
           version="3.0">
    <servlet>
        <servlet-name>about</servlet-name>
        <jsp-file>/jsp/about.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>contact</servlet-name>
        <jsp-file>/jsp/contact.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>learn</servlet-name>
        <jsp-file>/jsp/learn.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>index</servlet-name>
        <jsp-file>/jsp/index.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>likes</servlet-name>
        <jsp-file>/jsp/likes.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <jsp-file>/jsp/login.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>cover.css</servlet-name>
        <servlet-class>com.mylife.webapp.CoverCSSServlet</servlet-class>
    </servlet>
    <!-- JSPC servlet mappings start -->

    <servlet>
        <servlet-name>org.apache.jsp.jsp.about_jsp</servlet-name>
        <servlet-class>org.apache.jsp.jsp.about_jsp</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.apache.jsp.jsp.contact_jsp</servlet-name>
        <servlet-class>org.apache.jsp.jsp.contact_jsp</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.apache.jsp.jsp.index_jsp</servlet-name>
        <servlet-class>org.apache.jsp.jsp.index_jsp</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.apache.jsp.jsp.learn_jsp</servlet-name>
        <servlet-class>org.apache.jsp.jsp.learn_jsp</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.apache.jsp.jsp.likes_jsp</servlet-name>
        <servlet-class>org.apache.jsp.jsp.likes_jsp</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.apache.jsp.jsp.login_jsp</servlet-name>
        <servlet-class>org.apache.jsp.jsp.login_jsp</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.apache.jsp.jsp.includes.foot_jsp</servlet-name>
        <servlet-class>org.apache.jsp.jsp.includes.foot_jsp</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.apache.jsp.jsp.includes.head_jsp</servlet-name>
        <servlet-class>org.apache.jsp.jsp.includes.head_jsp</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.jsp.about_jsp</servlet-name>
        <url-pattern>/jsp/about.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.jsp.contact_jsp</servlet-name>
        <url-pattern>/jsp/contact.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.jsp.index_jsp</servlet-name>
        <url-pattern>/jsp/index.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.jsp.learn_jsp</servlet-name>
        <url-pattern>/jsp/learn.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.jsp.likes_jsp</servlet-name>
        <url-pattern>/jsp/likes.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.jsp.login_jsp</servlet-name>
        <url-pattern>/jsp/login.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.jsp.includes.foot_jsp</servlet-name>
        <url-pattern>/jsp/includes/foot.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.jsp.includes.head_jsp</servlet-name>
        <url-pattern>/jsp/includes/head.jsp</url-pattern>
    </servlet-mapping>

<!-- JSPC servlet mappings end -->

<servlet-mapping>
        <servlet-name>cover.css</servlet-name>
        <url-pattern>/css/cover.css</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>about</servlet-name>
        <url-pattern>/about</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>contact</servlet-name>
        <url-pattern>/contact</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>learn</servlet-name>
        <url-pattern>/learn</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>likes</servlet-name>
        <url-pattern>/likes</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index</welcome-file>
    </welcome-file-list>
</web-app>
